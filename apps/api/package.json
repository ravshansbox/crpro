{
  "name": "@cloudretail/api",
  "private": true,
  "types": "./src/types.ts",
  "scripts": {
    "docker:compose:up": "docker compose up",
    "prisma:generate": "prisma generate",
    "start:server": "nodemon src",
    "start": "npm-run-all -s prisma:generate start:server"
  },
  "dependencies": {
    "@prisma/client": "^5.9.1",
    "@trpc/server": "^10.45.1",
    "date-fns": "^3.3.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/node": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
    "@types/node": "^20.11.17",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "nodemon": "^3.0.3",
    "npm-run-all": "^4.1.5",
    "prisma": "^5.9.1",
    "typescript": "^5.3.3"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-typescript"
    ]
  },
  "nodemonConfig": {
    "exec": "babel-node --extensions .ts",
    "ext": "ts",
    "watch": [
      "src"
    ]
  }
}
